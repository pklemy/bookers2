
 <%= render 'tenplate',user:@user %>
 
 <table>
 <h1>Books</h1>
  <thead>
    <tr>
      <th>Title</th>
      <th>Opinion</th>
    </tr>
  </thead>
  <% @books.each do |book| %>
  <tr>
   <td><%= link_to user_path(book.user) do %>
   <%= image_tag book.user.get_profile_image(100, 100) %>
   <% end %>
   </td>
   <td><%= link_to book.title ,book_path(book.id)%></td>
   <td><%= book.body %></td>
   <% end %>
 </tr>
</table>
