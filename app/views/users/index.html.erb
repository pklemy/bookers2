<div>
  <%= flash[:notice] %>
</div>

<div class='container px-5 px-sm-0'>
<div class='row'>
<div class='col-md-3'>
<%= render 'books/tenplate',user: @user, book: @book %>
</div>

<div class='col-md-8 offset-md-1'>
 <h1>Users</h1>
 <table class="table">
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
    </tr>
  </thead>
  
  <tbody>
  <% @users.each do |user| %>
  <tr>
   <td><%= link_to users_path do %>
   <%= image_tag user.get_profile_image(100, 100) %>
   <% end %>
   </td>
   <!--<td><%#= link_to book.title ,book_path(book.id)%></td>-->
   <td><%= user.name %></td>
   <td><%= link_to "Show", user_path(user.id) %></td>
   <% end %>
 </tr>
 </tbody>
</table>
</div>
</div>
</div>